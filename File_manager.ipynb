      "source": [
        "This block to detect is the video has sub file and vice versa."
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "KaDBzg1Qpg2q",
        "cellView": "form"
      },
      "source": [
        "import os\n",
        "import shutil\n",
        "import time\n",
        "from pathlib import Path\n",
        "\n",
        "import numpy as np\n",
        "\n",
        "# @markdown Moving non subbed video\n",
        "SOURCE = \"/content/drive/Shared drives/Anime/judas\"  # @param {type:\"string\"}\n",
        "DESTINATION = \"/content/drive/Shared drives/Anime/duplicate\"  # @param {type:\"string\"}\n",
        "SOURCE = SOURCE if SOURCE.endswith(\"/\") else f\"{SOURCE}/\"\n",
        "DESTINATION = DESTINATION if DESTINATION.endswith(\"/\") else f\"{DESTINATION}/\"\n",
        "\n",
        "video = []\n",
        "subs_ass = []\n",
        "subs_sup = []\n",
        "buff = []\n",
        "diff = []\n",
        "BEGIN = time.time()\n",
        "error = False\n",
        "\n",
        "if not os.path.isdir(DESTINATION):\n",
        "    os.mkdir(DESTINATION)\n",
        "\n",
        "for r, d, f in os.walk(SOURCE, topdown=True):\n",
        "    for name in f:\n",
        "        if \".mp4\" in name:\n",
        "            video.append(os.path.join(r, name.split('.')[0]))\n",
        "        elif \".ass\" in name:\n",
        "            subs_ass.append(os.path.join(r, name.split('.')[0]))\n",
        "        elif \".sup\" in name:\n",
        "            subs_sup.append(os.path.join(r, name.split('.')[0]))\n",
        "\n",
        "buff = np.setdiff1d(video, subs_ass)\n",
        "dif_vid = np.setdiff1d(buff, subs_sup)\n",
        "\n",
        "dif_ass = np.setdiff1d(subs_ass, video)\n",
        "dif_sup = np.setdiff1d(subs_sup, video)\n",
        "\n",
        "\n",
        "for file_name in dif_vid:\n",
        "    vid_name = f\"{file_name}.mp4\"\n",
        "    vid_destination = f\"{vid_name.replace(SOURCE, DESTINATION)}\"\n",
        "    # print(f'y = {y}\\nz = {z}')\n",
        "    dst = Path(vid_destination).parents\n",
        "    for folder_path in reversed(dst):\n",
        "        if not os.path.isdir(folder_path):\n",
        "            os.mkdir(folder_path)\n",
        "    shutil.move(vid_name, dst[0])\n",
        "\n",
        "\n",
        "for file_name in dif_ass:\n",
        "    vid_name = f\"{file_name}.ass\"\n",
        "    vid_destination = f\"{vid_name.replace(SOURCE, DESTINATION)}\"\n",
        "    dst = Path(vid_destination).parents\n",
        "    for folder_path in reversed(dst):\n",
        "        if not os.path.isdir(folder_path):\n",
        "            os.mkdir(folder_path)\n",
        "    shutil.move(vid_name, dst[0])\n",
        "\n",
        "\n",
        "for file_name in dif_sup:\n",
        "    vid_name = f\"{file_name}.sup\"\n",
        "    vid_destination = f\"{vid_name.replace(SOURCE, DESTINATION)}\"\n",
        "    dst = Path(vid_destination).parents\n",
        "    for folder_path in reversed(dst):\n",
        "        if not os.path.isdir(folder_path):\n",
        "            os.mkdir(folder_path)\n",
        "    shutil.move(vid_name, dst[0])\n"
      ],
