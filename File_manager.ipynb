      "source": [
        "## Copy files from dir to dir"
      ]
    },
    {
      "cell_type": "markdown",
      "metadata": {
        "id": "x4ovmmbV9S_E"
      },
      "source": [
        "Copying/moving files in google drive just like copying file in windows explorer."
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "Yo3fCIoN8PAu",
        "cellView": "form"
      },
      "source": [
        "import os\n",
        "import shutil\n",
        "import threading\n",
        "import time\n",
        "\n",
        "import numpy as np\n",
        "\n",
        "# @markdown Copying files from dir to dir\n",
        "SOURCE = \"/content/drive/Shareddrives/Anime/converted h.256 (HEVC)\"  # @param {type:\"string\"}\n",
        "DESTINATION = \"/content/drive/Shareddrives/Anime Backup\"  # @param {type:\"string\"}\n",
        "SOURCE = SOURCE if SOURCE.endswith(\"/\") else f\"{SOURCE}/\"\n",
        "DESTINATION = DESTINATION if DESTINATION.endswith(\"/\") else f\"{DESTINATION}/\"\n",
        "DO = \"copy\"  # @param [\"copy\", \"move\"]\n",
        "# @markdown Stop on error\n",
        "STOP = True  # @param {type:\"boolean\"}\n",
        "\n",
        "files = []\n",
        "copied = []\n",
        "make = []  # dirs structure to create on destination\n",
        "fails = []  # failed files\n",
        "BEGIN = time.time()\n",
        "error = False\n",
        "\n",
        "def action(src_file, dest_dir):\n",
        "    global waiting\n",
        "    try:\n",
        "        if DO == \"copy\":\n",
        "            shutil.copyfile(src_file, dest_dir)\n",
        "        elif DO == \"move\":\n",
        "            shutil.move(src_file, dest_dir)\n",
        "    except:\n",
        "        if STOP:\n",
        "            global error\n",
        "            error = True\n",
        "    waiting = False\n",
        "\n",
        "\n",
        "def duration(num, msg=\"ETA... \", sep=\":\"):\n",
        "    if int((num - BEGIN) // 3600) > 0:\n",
        "        msg += f\"{int((num - BEGIN) // 3600)}{sep}\"\n",
        "    if int((num - BEGIN) // 60) > 0:\n",
        "        msg += f\"{int((num - BEGIN) // 60) % 60}{sep}\"\n",
        "    msg += f'{int((num - BEGIN) % 60)} sec'\n",
        "    return msg\n",
        "\n",
        "\n",
        "def finish():\n",
        "    from google.colab import output\n",
        "    output.eval_js(\n",
        "        'new Audio(\"https://drive.google.com/uc?export=download&id=1h-KhKaO-FNMsRKKYtiZ-AqNy8tr5mHcX\").play()')\n",
        "\n",
        "\n",
        "announce_text = f\"Detecting files (nothing to show to prevent lag)\"\n",
        "print(announce_text, end='')\n",
        "\n",
        "if not os.path.isdir(DESTINATION):\n",
        "    os.mkdir(DESTINATION)\n",
        "\n",
        "for r, d, f in os.walk(SOURCE, topdown=True):\n",
        "    for name in f:\n",
        "        files.append(os.path.join(r, name))\n",
        "    for name in d:\n",
        "        make.append(os.path.join(r, name))\n",
        "buff = []\n",
        "for r, d, f in os.walk(DESTINATION, topdown=True):\n",
        "    for file_name in f:\n",
        "        buff.append(os.path.join(r, file_name))\n",
        "\n",
        "for idx, fail in enumerate(buff):\n",
        "    copied.append(fail.replace(DESTINATION, SOURCE))\n",
        "\n",
        "del buff  # just for prevent if any other buff name will used\n",
        "\n",
        "if DO == \"move\":\n",
        "    delete_files = np.intersect1d(files, copied)\n",
        "    for file_name in delete_files:\n",
        "        try:\n",
        "            os.remove(file_name.tostring())\n",
        "        except FileNotFoundError:  # skip non existing file\n",
        "            pass\n",
        "\n",
        "files = np.setdiff1d(files, copied)\n",
        "\n",
        "print(\"\\b\" * len(announce_text), end='')\n",
        "announce_text = f\"All files detected\"\n",
        "print(announce_text, end='\\n')\n",
        "\n",
        "announce_text = f\"Creating Structure : \"\n",
        "print(announce_text, end='')\n",
        "mem = len(announce_text)\n",
        "mxm = len(make)\n",
        "\n",
        "for idx, path in enumerate(make):\n",
        "    child = path.replace(SOURCE, DESTINATION)\n",
        "    if not os.path.isdir(child):\n",
        "        time_elapse = duration(time.time(), \"Time Elapsed\")\n",
        "        announce_text = f\"{path.replace(SOURCE, '')}\\nCreated {idx} of {mxm}\\n{time_elapse}\"\n",
        "        print(announce_text, end='')\n",
        "        os.mkdir(child)\n",
        "        print(\"\\b\" * len(announce_text), end='')\n",
        "else:\n",
        "    print(\"\\b\" * mem, end='')\n",
        "    print(f\"All structure created\")\n",
        "\n",
        "\n",
        "announce_text = f\"Copying : \"\n",
        "print(announce_text, end='')\n",
        "mem = len(announce_text)\n",
        "mxm = len(files)\n",
        "\n",
        "for idx, src_file in enumerate(files):\n",
        "    dest_dir = src_file.replace(SOURCE, DESTINATION)\n",
        "    announce_text = f\"{str(src_file).split('/')[-1:][0]}\\nCopied {idx} of {mxm}\\n\"\n",
        "    print(announce_text, end='')\n",
        "\n",
        "    t1 = threading.Thread(target=action, args=(src_file, dest_dir), daemon=False)\n",
        "    waiting = True\n",
        "    t1.start()\n",
        "    announce_time_left_text = \"\"  # it could be undefined\n",
        "    while waiting:\n",
        "        announce_time_left_text = f\"{duration(time.time(), 'Time Elapsed: ')}\"  # Duration\n",
        "        print(f'\\r{announce_time_left_text}', end='')\n",
        "        time.sleep(1)\n",
        "    if error:\n",
        "        print(\"\\nAn Error founded. copy the output and share to the author\\n\")\n",
        "        raise SystemExit()\n",
        "    print(\"\\b\" * len(announce_time_left_text), end='')\n",
        "    print(\"\\b\" * len(announce_text), end='')\n",
        "    if not os.path.isfile(dest_dir):\n",
        "        fails.append(src_file)\n",
        "else:\n",
        "    print(\"\\b\" * mem, end='')\n",
        "    print(f\"All files copied\")\n",
        "\n",
        "if fails:\n",
        "    print(\"There's failed file(s):\")\n",
        "    for fail in fails:\n",
        "        print(fail.split('/')[-1:][0])\n",
        "\n",
        "t1 = threading.Thread(target=finish, args=(), daemon=False)\n",
        "t1.start()\n",
        "time.sleep(5)\n",
        "if t1.is_alive():\n",
        "    print(\n",
        "        \"\\nYOU CAN IGNORE ANYTHING BELOW THIS.\\nTHIS IS JUST TO FORCE STOP THE PROGRAM DUE AUDIO NOTIFICATION ERROR\\n\\n\")\n",
        "    raise SystemExit()  # sometime it got stuck when playing the notif audio.\n"
      ],
      "execution_count": null,
      "outputs": []
    },
